_id":"host_meta/Object","kind":"class","full_name":"Object","name":"Object"}],"locations":[{"filename":"src/host_meta/result.cr","line_number":10,"url":"https://github.com/toddsundsted/host_meta/blob/v0.2.2/src/host_meta/result.cr#L10"}],"repository_name":"host_meta","program":false,"enum":false,"alias":false,"aliased":null,"aliased_html":null,"const":false,"constants":[],"included_modules":[{"html_id":"host_meta/JSON/Serializable","kind":"module","full_name":"JSON::Serializable","name":"Serializable"}],"extended_modules":[],"subclasses":[],"including_types":[],"namespace":{"html_id":"host_meta/HostMeta","kind":"module","full_name":"HostMeta","name":"HostMeta"},"doc":"A `HostMeta` query result.","summary":"<p>A <code><a href=\"../HostMeta.html\">HostMeta</a></code> query result.</p>","class_methods":[{"id":"from_xml(xml)-class-method","html_id":"from_xml(xml)-class-method","name":"from_xml","doc":null,"summary":null,"abstract":false,"args":[{"name":"xml","doc":null,"default_value":"","external_name":"xml","restriction":""}],"args_string":"(xml)","args_html":"(xml)","location":{"filename":"src/host_meta/result.cr","line_number":35,"url":"https://github.com/toddsundsted/host_meta/blob/v0.2.2/src/host_meta/result.cr#L35"},"def":{"name":"from_xml","args":[{"name":"xml","doc":null,"default_value":"","external_name":"xml","restriction":""}],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"","visibility":"Public","body":"xml = (XML.parse(xml)).first_element_child\nif xml\nelse\n  raise(ResultError.new(\"empty result\"))\nend\nns = xml.namespaces\nxrd = (begin\n  key = (ns.key_for?(\"http://docs.oasis-open.org/ns/xri/xrd-1.0\")).try(&.split(\":\")).try(&.last)\n  key ? \"#{key}:\" : \"\"\nend)\nif (nn = xml.xpath_nodes(\"./#{xrd}Property\", ns)).size > 0\n  properties = nn.reduce(Hash(String, String | ::Nil).new) do |acc, n|\n    acc[n[\"type\"]] = n[\"nil\"]? ? nil : n.content\n    acc\n  end\nend\nif (nn = xml.xpath_nodes(\"./#{xrd}Link\", ns)).size > 0\n  links = nn.reduce(Array(Link).new) do |acc, n|\n    acc << (Link.from_xml(n))\n    acc\n  end\nend\nnew(properties, links)\n"}}],"constructors":[{"id":"new(properties,links)-class-method","html_id":"new(properties,links)-class-method","name":"new","doc":null,"summary":null,"abstract":false,"args":[{"name":"properties","doc":null,"default_value":"","external_name":"properties","restriction":""},{"name":"links","doc":null,"default_value":"","external_name":"links","restriction":""}],"args_string":"(properties, links)","args_html":"(properties, links)","location":{"filename":"src/host_meta/result.cr","line_number":32,"url":"https://github.com/toddsundsted/host_meta/blob/v0.2.2/src/host_meta/result.cr#L32"},"def":{"name":"new","args":[{"name":"properties","doc":null,"default_value":"","external_name":"properties","restriction":""},{"name":"links","doc":null,"default_value":"","external_name":"links","restriction":""}],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"","visibility":"Public","body":"_ = allocate\n_.initialize(properties, links)\nif _.responds_to?(:finalize)\n  ::GC.add_finalizer(_)\nend\n_\n"}},{"id":"new(pull:JSON::PullParser)-class-method","html_id":"new(pull:JSON::PullParser)-class-method","name":"new","doc":null,"summary":null,"abstract":false,"args":[{"name":"pull","doc":null,"default_value":"","external_name":"pull","restriction":"::JSON::PullParser"}],"args_string":"(pull : JSON::PullParser)","args_html":"(pull : JSON::PullParser)","location":{"filename":"src/host_meta/result.cr","line_number":27,"url":"https://github.com/toddsundsted/host_meta/blob/v0.2.2/src/host_meta/result.cr#L27"},"def":{"name":"new","args":[{"name":"pull","doc":null,"default_value":"","external_name":"pull","restriction":"::JSON::PullParser"}],"double_splat":null,"splat_index":null,"yields":null,"block_arg":null,"return_type":"","visibility":"Public","body":"new_from_json_pull_parser(pull)"}}],"instance_methods":[{"id":